#facts {
height: 320px;
overflow-x:hidden;
overflow-y:visible;
}
#facts > div {
color:#603F9D;
}
#facts > div > div {
padding:2px 0;
}
#facts > div li {
list-style-image:none;
list-style-position:outside;
list-style-type:none;
margin:0 0 0 -20px;
padding:2px 8px;
}
#complete .check_div, #totals_by_section > div {
float:left;
margin-right:8px;
position:relative;
}

#facts .facts_tools {
background:#D2C1F2 none repeat scroll 0 0;
width:100%;
}

#facts .facts_tools > div {
display: table-row;
}

#facts .facts_tools div > *:not(input) {
 margin: 6px 4px;
 float: left;
}
#facts .facts_tools div > input {
float: left;
margin: 2px 0px;
}
#facts .facts_tools > div > span {
cursor:pointer;
margin: 2px 0px;
}
